{
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier"],
  "plugins": ["jsx-a11y", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "react/no-unescaped-entities": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": 0
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
